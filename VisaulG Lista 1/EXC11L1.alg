algoritmo "semnome"

var
  p1,p2,p3,media:real

inicio
  escreval("qual a nota da primeira prova?")
  leia(p1)
  escreval("qual a nota da segunda prova?")
  leia(p2)
  escreval("qual a nota da terceira prova?")
  leia(p3)
  
  media<-(p1+p2+p3)/3
  
  escreval
  escreval("a media das suas três provas é:",media)
fimalgoritmo
