algoritmo "volume do prisma"

var
  l1,l2,l3,volume_do_prisma:real
inicio
  escreval("qual o valor do primeiro lado?")
  leia(l1)
  escreval("qual o valor do segundo lado?")
  leia(l2)
  escreval("qual o valor do terceiro lado?")
  leia(l3)
  
  volume_do_prisma<-l1*l2*l3
  
  escreval
  escreval("o volume do prisma é:",volume_do_prisma)
fimalgoritmo
