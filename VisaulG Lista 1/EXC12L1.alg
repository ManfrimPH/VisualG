algoritmo "tofu"

var
  valor1,valor2,valor3,valor_total:real
inicio
  escreval("qual o primeiro valor?")
  leia(valor1)
  escreval("qual o segundo valor?")
  leia(valor2)
  escreval("qual o terceiro valor?")
  leia(valor3)
  
  valor_total<-valor1*valor2*valor3
  
  escreval
  escreval("O valor da multiplicação dos três valores é:",valor_total)
fimalgoritmo
