algoritmo "semnome"

var
n1,n2:real

inicio

escreval("Me de um valor:")
leia(n1)

escreval("Me de outro valor:")
leia(n2)


se (n1>n2) entao
   escreval("Sua subtração é: ",n1-n2)
   escreval("Seu produto é: ",n1*n2)
senao
     escreval("Sua subtração é: ",n2-n1)
     escreval("Seu produto é: ",n2*n1)
fimse


fimalgoritmo
