algoritmo "semnome"

var
n1,n2:real
inicio
escreval("Me fale um número:")
leia(n1)
escreval("Agora me fale um segundo número diferente do primeiro:")
leia(n2)
se (n1>n2) entao
   escreval("O primeiro valor é maior do que o segundo")
senao
   escreval("O segundo valor é maior do que o primeiro")
fimse
fimalgoritmo
